<h1><%= category.name %></h1>
<p><%= category.description %></p>

<h2>Jogos</h2>

<ul>
  <% games.forEach(game => { %>
    <li>
      <a href="/games/<%= game.id %>">
        <%= game.title %>
      </a>
      - R$ <%= game.price %>
    </li>
  <% }) %>
</ul>

<a href="/games/new">Adicionar Jogo</a>